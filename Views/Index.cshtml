@{
}

<h1>Profile Details: </h1>

<div>
    <span>Username: @Model.Username </span>

    <h5>Published Books: </h5>
    <ul>
        @foreach (var book in ViewBag.Books)
        {
            <li class="text-lg-start"><a asp-action="Details" asp-controller="Book">@book.BookName</a></li>
        }
</div>